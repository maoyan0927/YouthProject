<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.youth.mapper.YouthInfoMapper">
    <select id="getYouthInfoByUserId" resultType="com.youth.Entity.YouthInfo">
        SELECT * FROM youth_info WHERE youth_info.user_id = #{userId}
    </select>
</mapper>