<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.youths.mapper.UserMapper">

    <resultMap id="LabelConsumerResultMap" type="com.youths.domain.LabelConsumer">
        <id column="consumer_id" property="consumerId" jdbcType="INTEGER"/>
        <result column="consumer_name" property="consumerName" jdbcType="VARCHAR"/>
        <result column="consumer_phone" property="consumerPhone" jdbcType="VARCHAR"/>
        <result column="password" property="password" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
        <result column="kind" property="kind" jdbcType="INTEGER"/>
        <result column="state" property="state" jdbcType="INTEGER"/>
    </resultMap>



</mapper>